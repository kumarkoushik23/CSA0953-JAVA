public class MaxElementInArray {
    public static void main(String[] args) {
        int[] array = {34, 12, 45, 9, 67};
        int max = array[0];

        for (int num : array) {
            if (num > max) {
                max = num;
            }
        }

        System.out.println("Maximum element in the array: " + max);
    }
}
