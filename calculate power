public class CalculatePower {
    public static void main(String[] args) {
        int base = 2;
        int exponent = 5;
        long result = 1;

        for (int i = 0; i < exponent; i++) {
            result *= base;
        }

        System.out.println(base + " ^ " + exponent + " = " + result);
    }
}
